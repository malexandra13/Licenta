<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    app:startDestination="@id/nav_home">
    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.licenta.client.fragments.HomeFragment"
        android:label="Acasă"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/nav_account"
        android:name="com.example.licenta.client.fragments.ProfileFragment"
        android:label="Profil"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/nav_about"
        android:name="com.example.licenta.client.fragments.AboutFragment"
        android:label="Despre noi"
        tools:layout="@layout/fragment_about" />
    <fragment
        android:id="@+id/nav_inbox"
        android:name="com.example.licenta.client.fragments.NextAppointmentsFragment"
        android:label="Urmatoarele programări"
        tools:layout="@layout/fragment_next_appointments" />
    <fragment
        android:id="@+id/historyAppointmentsFragment"
        android:name="com.example.licenta.client.fragments.HistoryAppointmentsFragment"
        android:label="Istoric programări"
        tools:layout="@layout/fragment_history" />
